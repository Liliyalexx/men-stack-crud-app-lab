<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=h1, initial-scale=1.0">
    <title>Document</title>
</head>
<!-- views/planets/index.ejs -->

<body>
    <h1>All planets</h1>
    <nav>
        <ul>
            <li>
                <a href="/planets/new" > Add new Planet</a>
            </li>
        </ul>
    </nav>
    <ul>
      <% planets.forEach(planet => { %>
        <li>
            <a href="/planets/<%= planet._id %>"> 
                <%= planet.name %>
            </a>
            </li>
      <% }); %>
    </ul>
  </body>
  
  
</html>